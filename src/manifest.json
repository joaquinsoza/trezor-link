{
  "name": "@trezor/link",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["scripting", "storage"],
  "host_permissions": [
    "*://connect.trezor.io/9/*",
    "*://suite.corp.sldev.cz/*",
    "*://localhost/*",
    "*://staging-connect.trezor.io/*",
    "*://dev.suite.sldev.cz/*"
  ]
}
